<div class="card border-0 shadow-none">
    {{ form_start(form) }}
    <div class="card-body p-2">
        <div class="row">
            <div class="col-md-12">
                {{ form_row(form.name) }}
            </div>

            <div class="col-md-2 text-start">
                <button class="btn btn-primary w-100" id="save-btn">{{ show_submit_label|default('Create Action Queue Cron Group') }}</button>
            </div>
            {{ form_end(form) }}
        </div>
    </div>
</div>

